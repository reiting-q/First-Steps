   "source": [
    "countries_total = count_100.nlargest(10).sum()\n",
    "\n",
    "country_pct = top_10_countries / countries_total * 100\n",
    "\n",
    "country_pct = round(country_pct, 2)\n",
    "\n",
    "country_pct = pd.DataFrame(country_pct)\n",
    "\n",
    "country_pct.reset_index().rename(columns = {'index':'country', 'country':'% of unis in top 100'})\n",
    "\n",
    "# The percentage composition of each country's top 100 university are as follow."

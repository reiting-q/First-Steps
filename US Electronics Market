    "key_web = ['bluetooth','portable','headset','headphone', 'outdoor', 'outdoors', 'wireless bluetooth', 'audio']\n",
    "\n",
    "def online(df):\n",
    "    for i in df.index:\n",
    "        store = df.at[i,'cat_in_name']\n",
    "        for j in key_web:\n",
    "            if any(j in store for j in key_web):\n",
    "                df.at[i,'portable'] = 1\n",
    "            else:\n",
    "                df.at[i,'portable'] = 0\n",
    "        \n",
    "online(data4)\n",
    "data4"
   ]

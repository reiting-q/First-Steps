    "key_web = ['tv','home','4k','projector','smart','system','subwoofer','mount','theater','audio','sound']\n",
    "\n",
    "def online(df):\n",
    "    for i in df.index:\n",
    "        store = df.at[i,'cat_in_name']\n",
    "        for j in key_web:\n",
    "            if any(j in store for j in key_web):\n",
    "                df.at[i,'home'] = 1\n",
    "            else:\n",
    "                df.at[i,'home'] = 0\n",
    "        \n",
    "online(data4)\n",
    "data4"

   "cell_type": "code",
   "execution_count": 129,
   "metadata": {},
   "outputs": [],
   "source": [
    "result = xgb_sklearn.predict(X_test_s)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 130,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "RMSE = 384.328 \n",
      "MAE = 240.64689889534256 \n",
      "R2 = 0.6988278090517804 \n",
      "Adjusted R2 = 0.690057880196044\n"
     ]
    }
   ],
   "source": [
    "RMSE = float(format(np.sqrt(mean_squared_error(y_test, result)) , '.3f'))\n",
    "\n",
    "MAE = mean_absolute_error(y_test, result)\n",
    "\n",
    "# Adjusted R2 'penalizes' you for adding features that are not contributing much to the model \n",
    "# R2 assumes that every single variable explains the variation in the dependent variable. \n",
    "# The adjusted R2 tells you the percentage of variation explained by independent variables that actually affect the y.\n",
    "\n",
    "r2 = r2_score(y_test, result)\n",
    "adj_r2 = 1-(1-r2)*(n-1)/(n-k-1)   # no inbuilt function for r2 adjusted so need to code the formula out\n",
    "\n",
    "print('RMSE =',RMSE, '\\nMAE =',MAE, '\\nR2 =', r2, '\\nAdjusted R2 =', adj_r2) \n",
    "\n",
    "# Adjusted R2 is lower than R2 because it adjusts for larger errors, and usually tends to be lower"
   ]
  },

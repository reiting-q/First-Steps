    "key_web = ['camera','lens','mirrorless']\n",
    "\n",
    "def online(data4):\n",
    "    for i in data4.index:\n",
    "        store = data4.at[i,'cat_in_name']\n",
    "        for j in key_web:\n",
    "            if any(j in store for j in key_web):\n",
    "                data4.at[i,'camera'] = 1\n",
    "            else:\n",
    "                data4.at[i,'camera']=0\n",
    "        \n",
    "online(data4)\n",
    "data4"
   ]

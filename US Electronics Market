  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1.0    0.699131\n",
       "0.0    0.300869\n",
       "Name: online, dtype: float64"
      ]
     },
     "execution_count": 16,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "new_data2 = new_data1.copy()\n",
    "\n",
    "key_web = ['.com','-com']\n",
    "\n",
    "def online(df):\n",
    "    for i in df.index:\n",
    "        store = df.at[i,'seller']\n",
    "        for j in key_web:\n",
    "            if any(j in store for j in key_web):\n",
    "                df.at[i,'online'] = 1\n",
    "            else:\n",
    "                df.at[i,'online'] = 0\n",
    "        \n",
    "online(new_data2)\n",
    "\n",
    "new_data2.online.value_counts(normalize = True)"
   ]
  },

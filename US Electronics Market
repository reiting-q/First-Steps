   "source": [
    "key_web = ['walmart']\n",
    "\n",
    "def online(data6):\n",
    "    for i in data6.index:\n",
    "        store = data6.at[i,'seller']\n",
    "        for j in key_web:\n",
    "            if any(j in store for j in key_web):\n",
    "                data6.at[i,'walmart']=1\n",
    "            else:\n",
    "                data6.at[i,'walmart']=0\n",
    "        \n",
    "online(data6)\n",
    "data6"
   ]

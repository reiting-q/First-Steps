    "# Fit the linear regression model to train set\n",
    "\n",
    "model_LR = LinearRegression()\n",
    "model_LR.fit(X_train , y_train)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 100,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([371.17897034, 165.77224731, 209.89157104, ..., 261.35993958,\n",
       "        33.88221741, 102.02258301])"
      ]
     },
     "execution_count": 100,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# predict on test set using X_test to get predicted y values\n",
    "\n",
    "LR_y = model_LR.predict(X_test)\n",
    "LR_y"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 101,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "RMSE = 528.822 \n",
      "MAE = 315.55903135691676 \n",
      "R2 = 0.42979765147104343 \n",
      "Adjusted R2 = 0.41319374785620877\n"
     ]
    }
   ],
   "source": [
    "RMSE = float(format(np.sqrt(mean_squared_error(y_test, LR_y)) , '.3f'))\n",
    "\n",
    "MAE = mean_absolute_error(y_test, LR_y)\n",
    "\n",
    "# Adjusted R2 'penalizes' you for adding features that are not contributing much to the model \n",
    "# R2 assumes that every single variable explains the variation in the dependent variable. \n",
    "# The adjusted R2 tells you the percentage of variation explained by independent variables that actually affect the y.\n",
    "\n",
    "r2 = r2_score(y_test, LR_y)\n",
    "adj_r2 = 1-(1-r2)*(n-1)/(n-k-1)   # no inbuilt function for r2 adjusted so need to code the formula out\n",
    "\n",
    "print('RMSE =',RMSE, '\\nMAE =',MAE, '\\nR2 =', r2, '\\nAdjusted R2 =', adj_r2) \n",
    "\n",
    "# Adjusted R2 is lower than R2 because it adjusts for larger errors, and usually tends to be lower"
   ]
  },

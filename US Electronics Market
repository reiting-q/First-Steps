   "source": [
    "key_web = ['computer','laptop','drive','memory','monitor','desktop','tablet','internal',\n",
    "           'external','hard','storage','mouse','keyboard', 'wireless modem']\n",
    "\n",
    "def online(df):\n",
    "    for i in df.index:\n",
    "        store = df.at[i,'cat_in_name']\n",
    "        for j in key_web:\n",
    "            if any(j in store for j in key_web):\n",
    "                df.at[i,'computer']=1\n",
    "            else:\n",
    "                df.at[i,'computer']=0\n",
    "        \n",
    "online(data4)\n",
    "data4"
   ]
